{
    "manifest_version": 2,
    "name": "URL Shortener",
    "description": "An extension to shorten URLs using Bitly API",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "https://ismaelc-bitly.p.rapidapi.com/*"
    ],
    "icons": {
      "128": "link.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "link.png"
      }
    }
  }
  